OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DLImageLoader" -isystem "${PODS_ROOT}/Headers/Public/PHFComposeBarView" -isystem "${PODS_ROOT}/Headers/Public/PHFDelegateChain"
OTHER_LDFLAGS = -ObjC -l"Pods-conversation-DLImageLoader" -l"Pods-conversation-PHFComposeBarView" -l"Pods-conversation-PHFDelegateChain"
